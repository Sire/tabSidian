{
    "manifest_version": 2,
    "name": "tabSidian",
    "version": "1.5",
    "description": "A browser extension to summarize open tabs into Obsidian markdown files.",
    "icons": {
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "browser_action": {
      "default_icon": "icon48.png",
      "default_title": "tabSidian"
    },
    "permissions": [
      "tabs",
      "storage",
      "downloads"
    ],
    "options_page": "options.html",
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    }
  }
  